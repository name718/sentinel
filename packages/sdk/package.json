{
  "name": "@majuntao-1/sentinel-sdk",
  "version": "1.0.0",
  "description": "轻量级前端监控 SDK - 错误监控、性能分析、会话回放",
  "keywords": [
    "monitor",
    "frontend",
    "error-tracking",
    "performance",
    "web-vitals",
    "session-replay",
    "rrweb",
    "apm",
    "sentinel"
  ],
  "author": "majuntao",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/name718/sentinel"
  },
  "homepage": "https://github.com/name718/sentinel#readme",
  "bugs": {
    "url": "https://github.com/name718/sentinel/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "fast-check": "^3.14.0",
    "jsdom": "^27.3.0",
    "rollup": "^4.6.0",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "rrweb": "2.0.0-alpha.4",
    "rrweb-snapshot": "2.0.0-alpha.4"
  },
  "engines": {
    "node": ">=16"
  }
}
